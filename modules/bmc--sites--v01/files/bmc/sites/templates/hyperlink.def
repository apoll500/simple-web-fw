
DEF HYPERLINK
{
    DEF load_page
    {
        xLANG="@parent.LANG";
        xtitle="@<@parent.LANG>.title";
        DEF PRINT
        {
            ->
            <a class="@class" href="JavaScript:load_page('@token')">@xtitle</a>
            <--
        }
        DEF CTEXT
        {
            ->
            <a class="@class" href="index.php?page=@token&lang=@xLANG">@xtitle</a>
            <--
        }
        class="std";
    }
    DEF load_mods
    {
        xLANG="@parent.LANG";
        xtitle="@<@parent.LANG>.title";
        DEF PRINT
        {
            ->
            <a class="@class" href="JavaScript:mods_load('@module','@token')">@xtitle</a>
            <--
        }
        DEF CTEXT
        {
            ->
            <a class="@class" href="index.php?action=@module[]_@token&lang=@xLANG">@xtitle</a>
            <--
        }
        class="std";
    }
}
